{
  "electron": {
    "files": [{
      "expand": true,
      "cwd": "<%= build %>",
      "src": "**",
      "dest": "<%= appDist %>"
    }, {
      "expand": true,
      "cwd": "<%= appSrc %>",
      "src": "**",
      "dest": "<%= appDist %>"
    }]
  },
  "browser": {
    "files": [{
        "expand": true,
        "cwd": "<%= webSrc %>",
        "src": "**",
        "dest": "<%= webDist %>"
      }]
  },
  "web": {
    "files": [{
        "expand": true,
        "cwd": "<%= webSrc %>",
        "src": "**",
        "dest": "<%= build %>"
      }]
  }
}
